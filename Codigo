int main() {
int x = 17;
bool y = true;
if (x<=1) {
  y = false;

}
else {
    for (int i = 2; i <= x / 2; ++i) {
    if (x % i == 0) {
        y = false;
        break;

       }
    }
}
if (y == true) {
    printf("El numero %d es primo\n", x);
}
else {
  printf("El numero %d no es primo\n", x);

   }
}
